<%- include("partials/header") %>
<div class="contact">
  <h1>CONTACT</h1>
  <p><%= contactPageContent %></p>
  <h4>Social Media Links :</h4>

  <ul class="contact-list">
    <li>
      <a
        href="https://www.linkedin.com/in/siddhant-kankaria-9982691aa/"
        target="_blank"
        class="link"
        >Linkedin</a
      >
    </li>
    <li>
      <a
        href="https://github.com/siddhantkankaria123"
        class="link"
        target="_blank"
        >Github</a
      >
    </li>
    <li>
      <a
        href="mailto: siddhantkankaria122@gmail.com"
        class="link"
        target="_blank"
      >
        Email</a
      >
    </li>
  </ul>
</div>
<form action="/contact" method="post">
  <div class="forms">
    <input
      type="text"
      class="contact-input"
      name="Name"
      placeholder="Name"
      autocomplete="off"
      required
    />
    <input
      type="email"
      class="contact-input"
      name="email"
      placeholder="Enter Email"
      required
    />
    <textarea
      name="message"
      id=""
      cols="20"
      rows="10"
      placeholder="Enter Message"
      class="contact-input"
      required
    ></textarea>
  </div>
  <div class="contact-button">
    <button type="submit" class="btn btn-primary">Send</button>
  </div>
</form>

